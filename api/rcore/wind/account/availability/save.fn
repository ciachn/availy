
(sentinel::auth-required)
(import "lib/directives")

(shield::type field-date
    required true
    pattern date
)

(shield::validate
    (shield::field date-list
        json-load "POST"
        data (array
            (type field-date)
        )
    )
)

(d-set-object (session.user.user_id) "availability" (date-list))
(&)
